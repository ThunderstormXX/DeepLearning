    "from PIL import Image\n",
    "import numpy as np\n",
    "import torch\n",
    "\n",
    "tensor_pixels = torch.zeros(28,28)\n",
    "pixels_array = tensor_pixels.numpy()\n",
    "\n",
    "image = Image.fromarray((pixels_array * 255).astype('uint8'), mode='L')\n",
    "\n",
    "image.save('black_and_white_image.jpg')\n"
